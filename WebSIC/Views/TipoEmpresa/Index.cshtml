                @Html.DisplayFor(modelItem => item.Descricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Criacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Criador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Atualizacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Atualizador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ativo)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.IdTipoEmpresa }) |
                @Html.ActionLink("Details", "Details", new { id=item.IdTipoEmpresa }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.IdTipoEmpresa })
